<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: wheat;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.OrderManagerWindow">
   <children>
      <Button fx:id="issueOrder" layoutX="198.0" layoutY="90.0" mnemonicParsing="false" onAction="#clickIssueOrder" prefHeight="26.0" prefWidth="203.0" text="Выдача" />
      <Button fx:id="refundOrder" layoutX="198.0" layoutY="130.0" mnemonicParsing="false" onAction="#clickRefundOrder" prefHeight="26.0" prefWidth="203.0" text="Возврат" />
      <Button fx:id="showAllOrders" layoutX="198.0" layoutY="172.0" mnemonicParsing="false" onAction="#clickShowAllOrders" prefHeight="26.0" prefWidth="203.0" text="Просмотр всех заказов" />
      <Button fx:id="showOrders" layoutX="198.0" layoutY="209.0" mnemonicParsing="false" onAction="#clickShowOrders" text="Просмотр всех выданных заказов" />
      <Button fx:id="back" layoutX="198.0" layoutY="246.0" mnemonicParsing="false" onAction="#clickBack" prefHeight="26.0" prefWidth="203.0" text="Назад" />
   </children>
</AnchorPane>
