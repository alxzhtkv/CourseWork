<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AuthorizationWindow">
   <children>
      <CheckBox fx:id="adminCheck" layoutX="25.0" layoutY="356.0" mnemonicParsing="false" text="Администратор" />
      <Button fx:id="signInBttn" layoutX="266.0" layoutY="276.0" mnemonicParsing="false" onAction="#enterSignIN" prefHeight="26.0" prefWidth="67.0" text="Войти" />
      <PasswordField fx:id="passTextField" layoutX="225.0" layoutY="219.0" />
      <TextField fx:id="loginTextField" layoutX="226.0" layoutY="175.0" />
      <Text layoutX="216.0" layoutY="120.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Авторизация">
         <font>
            <Font size="28.0" />
         </font>
      </Text>
      <Button fx:id="backBttn" layoutX="267.0" layoutY="320.0" mnemonicParsing="false" onAction="#enterback" prefHeight="26.0" prefWidth="67.0" text="Назад" />
   </children>
</AnchorPane>
